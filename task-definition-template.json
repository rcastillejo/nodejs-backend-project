{
    "family": "example-ecs-app",
    "networkMode": "awsvpc",
    "containerDefinitions": [
      {
        "name": "node-app",
        "image": "${IMAGE_NAME}",
        "portMappings": [
          {
            "containerPort": 3000,
            "hostPort": 3000,
            "protocol": "tcp"
          }
        ],
        "essential": true,
        "environment": [
          {
            "name": "API_KEY",
            "value": "${API_KEY}"
          }
        ]
      }
    ],
    "requiresCompatibilities": [
      "EC2"
    ],
    "cpu": "256",
    "memory": "512"
  }
  